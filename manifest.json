{
  "name": "Tabbie",
  "description": "Tabbie aims to be the most customizable new tab extension ever made.",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "dist/tab.html"
  },
  "permissions": [
    "https://lobste.rs/*"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
